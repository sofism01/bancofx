<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="800.0" spacing="30.0" style="-fx-padding: 30" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.banco.controlador.PanelClienteControlador">
   <children>
      <Label text="Panel principal de administración">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label text="Bienvenido a su banco, aquí podrá ver sus transacciones" />
      <TableView prefHeight="282.0" prefWidth="716.0">
        <columns>
          <TableColumn prefWidth="140.0" text="Tipo" />
          <TableColumn prefWidth="140.0" text="Fecha" />
            <TableColumn prefWidth="140.0" text="Valor" />
            <TableColumn prefWidth="140.0" text="Usuario" />
            <TableColumn prefWidth="140.0" text="Categoría" />
        </columns>
      </TableView>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="85.0" style="-fx-background-color: #00807a; -fx-text-fill: white;" text="Cerrar sesión">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="85.0" style="-fx-background-color: #00807a; -fx-text-fill: white;" text="Consultar">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="85.0" style="-fx-background-color: #00807a; -fx-text-fill: white;" text="Transferir">
               <HBox.margin>
                  <Insets right="200.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
